{
    "name": "ele-ui",
    "version": "1.0.7",
    "description": "A Component Library for Vue.js.",
    "main": "lib/ele-ui.common.js",
    "unpkg": "lib/index.js",
    "typings": "types/index.d.ts",
    "style": "lib/theme-chalk/index.css",
    "files": [
        "lib",
        "src",
        "packages",
        "types"
    ],
    "scripts": {
        "test:firefox": "cross-env browser=firefox karma start test/unit/karma.conf.js",
        "test:ie": "cross-env browser=ie karma start test/unit/karma.conf.js",
        "test:opera": "cross-env browser=opera karma start test/unit/karma.conf.js",
        "test:chrome": "cross-env browser=chrome karma start test/unit/karma.conf.js",
        "bootstrap": "yarn || npm i",
        "clean:demo": "rimraf dist",
        "clean:build": "rimraf lib",
        "demo": "npm run bootstrap && npm run clean:demo && webpack --config build/webpack.demo.js",
        "dev": "npm run bootstrap && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
        "build:component": "webpack --config build/webpack.component.js",
        "build:externals": "webpack --config build/webpack.utils.js && webpack --config build/webpack.enum.js",
        "build:common": "webpack --config build/webpack.common.js",
        "build:all": "npm run bootstrap && npm run clean:build && webpack --config build/webpack.build.js",
        "build:npm": "npm run bootstrap && npm run clean:build && npm run build:common && npm run build:component && npm run build:externals"
    },
    "dependencies": {
        "async-validator": "^3.2.2",
        "tslib": "^1.10.0",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.3.0"
    },
    "peerDependencies": {
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.0.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/plugin-syntax-jsx": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.0.0",
        "@babel/runtime-corejs2": "^7.7.4",
        "@types/mocha": "^5.2.7",
        "autoprefixer": "^9.7.2",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-vue-jsx": "4.0.1",
        "chai": "^3.5.0",
        "copy-webpack-plugin": "^5.0.5",
        "core-js": "^2.5.7",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.0",
        "dart-sass": "^1.23.3",
        "file-loader": "^4.3.0",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "happypack": "^5.0.1",
        "highlight.js": "^10.4.1",
        "html-webpack-plugin": "^3.2.0",
        "karma": "^1.3.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.2.0",
        "karma-ie-launcher": "^1.0.0",
        "karma-mocha": "^1.2.0",
        "karma-opera-launcher": "^1.0.0",
        "karma-sinon-chai": "^1.2.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.26",
        "karma-webpack": "^3.0.0",
        "markdown-it": "^10.0.0",
        "markdown-it-anchor": "^5.2.5",
        "markdown-it-chain": "^1.3.0",
        "markdown-it-container": "^2.0.0",
        "mini-css-extract-plugin": "^0.8.0",
        "mocha": "^3.1.1",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "progress-bar-webpack-plugin": "^1.12.1",
        "rimraf": "^3.0.0",
        "sass": "^1.23.3",
        "sass-loader": "^8.0.0",
        "sinon": "^1.17.6",
        "sinon-chai": "^2.8.0",
        "terser-webpack-plugin": "^2.2.1",
        "transliteration": "^2.1.7",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4",
        "url-loader": "^2.2.0",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.2",
        "vue-router": "^3.1.3",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "vue-tslint-loader": "^3.5.6",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
